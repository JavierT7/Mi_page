<div class="center jumbotron">
  <h1>Sign Up</h1>
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= form_for(@user) do |f| %>
          <%= render 'shared/error_messages' %>

          <%= f.label :name %>
          <%= f.text_field :name %>

          <%= f.label :email %>
          <%= f.text_field :email %>

          <%= f.submit "Sign Up", class: "btn btn-primaty" %>
      <% end %>
    </div>
  </div>
</div>